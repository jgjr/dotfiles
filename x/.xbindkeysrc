"urxvt"
  Control + Shift + Alt + Return

"chromium"
  Control + Shift + Alt + a

"chromium --incognito"
  Control + Shift + Alt + p

"rofi -show run"
  Control + Shift + Alt + d

"~/.config/i3lock/lock.sh"
  m:0x0 + c:202

"~/.config/i3lock/lock.sh; systemctl suspend"
  m:0x0 + c:201

"killall $BROWSER; killall vlc"
  m:0x0 + c:200

"urxvt -e ranger"
  Control + Shift + Alt + z

"Thunar"
  Control + Shift + Alt + x

"urxvt -e neomutt"
  Control + Shift + Alt + m

"urxvt -e khal interactive"
  Control + Shift + Alt + c

"keepassx2"
  Control + Shift + Alt + k

"gnome-calculator"
  Control + Shift + Alt + g

"$BROWSER ~/documents/character_codes.html"
  Control + Shift + Alt + t

"light -A 10"
  XF86MonBrightnessUp

"light -U 10"
  XF86MonBrightnessDown

"pactl set-sink-volume @DEFAULT_SINK@ +2000"
   XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -2000"
   XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

"~/bin/media-control playpause"
  XF86AudioPlay

"~/bin/media-control next"
  XF86AudioNext

"~/bin/media-control prev"
  XF86AudioPrev

"~/bin/media-control stop"
  XF86AudioStop

"scrot -d 0.25  -e 'mv $f ~/pictures/screenshots/'; notify-send 'Screenshot taken'"
  Print
